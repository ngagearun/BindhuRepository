<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="AutomationPractice" >
<listeners>
<listener class-name="com.testng.TestListener"></listener>

</listeners>
	<test name="ChromeBrowser">
	<parameter name="browsername" value="chrome" />
	<groups>
   <run>
    <include name="regression"></include>
   </run>  
  </groups>
		<classes>
			<class name="com.testng.CartSummaryPage"></class>
			<class name="com.testng.CustomerService"></class>
			<class name="com.testng.Dresses"></class>
			<class name="com.testng.Women"></class>
			<class name="com.testng.SelectDressThroughSearchBar"></class>
		</classes>
	</test>
	
	
	
</suite>


